{
    "manifest_version": 3,
    "version": "1.0",
    "name": "Quick Link",
    "action": {
        "default_popup": "../src/index.html"
    },
    "permissions": [
      "activeTab"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self' 'unsafe-inline' https://code.jquery.com; object-src 'self'"
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
}