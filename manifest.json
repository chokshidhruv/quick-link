{
    "manifest_version": 3,
    "version": "1.0",
    "name": "Quick Link",
    "action": {
        "default_popup": "../src/index.html"
    },
    "host-permisions": [
        ["tabs", "<all_urls>"]
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["jquery-3.7.1.min.js", "content.js"],
        "run_at": "document_start"
      }
    ]
}